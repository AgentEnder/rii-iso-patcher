{
  "projectType": "library",
  "sourceRoot": "libs/client/data-access/src",
  "prefix": "rii-iso-patcher",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/client/data-access"
      ],
      "options": {
        "jestConfig": "libs/client/data-access/jest.config.ts",
